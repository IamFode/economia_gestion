################ Estimadores de mínimos cuadrados ordinarios ###################
beta1 <- function(y,x){
sumnam <- 0
sumden <- 0
n = length(x)
n1 = length(y)
mediax = sum(x) / n
mediay = sum(y) / n1
for (i in 1:n){
sumnam <- sumnam + ( (x[i]-mediax) * (y[i]-mediay) )
sumden <- sumden + ( (x[i]-mediax)^2 )
}
beta1 <- sumnam / sumden
return(beta1)
}
beta0 <- function(y,x){
mediax = sum(x) / n
mediay = sum(y) / n1
beta0 <- mediay - beta1(y,x)*mediax
return(beta0)
}
